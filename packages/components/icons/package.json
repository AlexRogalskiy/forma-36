{
  "name": "@contentful/f36-icons",
  "version": "4.0.0",
  "description": "Forma 36: Icon components",
  "license": "MIT",
  "scripts": {
    "build": "node scripts/preBuild.js && yarn build:prod",
    "build:prod": "microbundle build --entry src/index.ts --sourcemap false --jsx React.createElement --output dist --format modern,esm,cjs --tsconfig ./tsconfig.build.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@contentful/f36-icon": "^4.0.0",
    "@contentful/f36-tokens": "^4.0.0",
    "@contentful/f36-core": "^4.0.0",
    "emotion": "^10.0.17"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": "./dist/f36-icons.modern.js",
  "main": "./dist/f36-icons.js",
  "module": "./dist/f36-icons.module.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "browserslist": "extends @contentful/browserslist-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/forma-36"
  },
  "publishConfig": {
    "access": "public"
  }
}
